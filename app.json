{
  "expo": {
    "name": "gastoscaculator",
    "slug": "gastoscaculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vougan.gastoscaculator"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vougan.gastoscaculator",
      "config": {
        "googleSignIn": {
          "apiKey": "buildkey"
        },
        "eas": {
          "runtimeVersion": "1.0.0",
          "build": {
            "android": {
              "release": {
                "keystore": "./buildkey.keystore",
                "keystoreAlias": "buildkey",
                "keystorePassword": "123456",
                "keyPassword": "123456"
              }
            }
          }
        }
      },
      "useNextNotificationsApi": true,
      "permissions": [],
      "googleServicesFile": "./google-services.json",
      "publishBundlePath": "android/app/.aab",
      "intentFilters": [
        {
          "action": "MAIN",
          "autoVerify": true,
          "category": [
            "ANDROID_APP_ACTION"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "28e27aa2-0645-4429-8baf-13f19d03ed85"
      }
    },
    "owner": "vougan"
  }
}
